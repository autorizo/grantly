{
  "name": "be",
  "version": "1.0.0",
  "scripts": {
    "dev": "cross-env NODE_PATH=./src nodemon --watch src --exec \"ts-node -r tsconfig-paths/register src/index.ts\"",
    "migrate": "ts-node src/db/migrate.ts",
    "rollback": "ts-node src/db/rollback.ts",
    "seed": "ts-node src/db/seed.ts",
    "start": "cross-env NODE_PATH=./src node -r tsconfig-paths/register src/index.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "knex": "^3.1.0",
    "module-alias": "^2.2.3",
    "mysql2": "^3.11.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.4",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.2"
  },
  "_moduleAliases": {
    "@db": "./src/db",
    "@controllers": "./src/controllers",
    "@routes": "./src/routes",
    "@knexfile": "./knexfile.js"
  }
}
